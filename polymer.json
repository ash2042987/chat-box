{
  "entrypoint": "index.html",
  "shell": "src/ceg-app.html",
  "fragments": [
    "src/ceg-views/ceg-chat.html",
    "src/ceg-views/ceg-login.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled",
      "basePath": true
    },
    {
      "preset": "es6-unbundled",
      "basePath": true
    }
  ]
}
